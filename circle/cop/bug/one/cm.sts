#/bin/sh

# dummy output

c=$1

out() {
echo ""
echo "=== start $1"
cat $1
echo "=== end $1"
rm -f $1
}

run(){
echo ""
echo "[$1 $2]"
$c $1 $2
if [ "$3" != "" ]; then
test -f $3 && out $3
fi
}

run -cc cm.cr
run -cm cm.cr.out
run -cv cm.cr.out.out

out cm.cr.out
out cm.cr.out.out
out cm.cr.out.out.out
